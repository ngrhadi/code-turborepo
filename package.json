{
	"name": "code-repo",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"buildAppWeb": "cd apps/web && yarn build",
		"buildAppDock": "cd apps/dock && yarn build",
		"build:vercel": "yarn buildAppWeb && buildAppDock"
	},
	"devDependencies": {
		"eslint-config-custom": "*",
		"prettier": "latest",
		"turbo": "^1.3.1"
	},
	"dependencies": {},
	"packageManager": "yarn@1.22.19"
}
